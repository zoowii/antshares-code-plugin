{
    "name": "antshares-code-plugin",
    "displayName": "antshares_code_plugin",
    "description": "",
    "version": "0.0.1",
    "publisher": "zoowii",
    "engines": {
        "vscode": "^1.13.0"
    },
    "categories": [
        "Snippets"
    ],
    "activationEvents": [
        "onCommand:extension.queryAnsBalance",
        "onLanguage:csharp"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [{
                "command": "extension.queryAnsBalance",
                "title": "antshares query balance"
            },
            {
                "command": "extension.compile_avm",
                "title": "antshares compile_avm"
            }
        ],
        "snippets": [{
            "language": "csharp",
            "path": "./snippets/antcsharp.json"
        }]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}